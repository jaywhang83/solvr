<div>
    <h1>Sign In</h1>
</div>
{{input
    type='text'
    class='form-control'
    value=email
    placeholder='email'
}}
{{input
    type='password'
    class='form-control'
    value=password
    placeholder='password'
}}
<button {{action 'signIn' 'password'}}>
    {{fa-icon 'sign-in'}} Sign In
</button>

{{#if session.isWorking}}
